      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.transportili.driver",
      "versionCode": 6,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBtgXAIMvXDVkoCBs2HwT2gi6p4-LLJiLI"
        }
      }
    },
    "description": "Simple login template created with React Native.",
    "githubUrl": "https://github.com/venits/react-native-login-template"
  }
